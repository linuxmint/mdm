#: ../daemon/auth.c:672
#, c-format
msgid "%s: Could not lock cookie file %s"
msgstr "%s: Неуспех при заключването на файл с бисквитка %s"

#: ../daemon/auth.c:721 ../daemon/auth.c:743
#, c-format
msgid "%s: Could not write cookie"
